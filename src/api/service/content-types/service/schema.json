{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": "KRA services and offerings"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "icon": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": ["individual", "business", "investor", "agent", "general"],
      "required": true
    },
    "isQuickLink": {
      "type": "boolean",
      "default": false
    },
    "externalUrl": {
      "type": "string"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "isActive": {
      "type": "boolean",
      "default": true
    }
  }
}
